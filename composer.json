{
    "name": "hallboav/datainfo-bundle",
    "type": "symfony-bundle",
    "description": "Datainfo Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hallison Boaventura",
            "email": "hallisonboaventura@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-xml": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/http-kernel": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/console": "^4.2",
        "symfony/config": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/cache": "^4.2",
        "monolog/monolog": "^1.24",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Hallboav\\DatainfoBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "phpmd": "vendor/bin/phpmd Client,Controller,DependencyInjection,Event,Security,Sistema,Tests html cleancode,codesize,controversial,design,naming,unusedcode > phpmd-report.html"
    }
}
